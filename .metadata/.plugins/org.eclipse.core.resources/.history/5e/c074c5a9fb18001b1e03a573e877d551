package Chapter13_2;


public class Quiz1306 {

	public static void main(String[] args) {
		int[][] arr = new int[3][9];
		
		for(int i = 0; i<3; i++) {
			for(int j= 0; j<9; j++) {
				int num[i][j] = i * j;
			}
		}
		
		for(int i = 0; i<3; i++) {
			for(int j= 0; j<9; j++) 
				System.out.println(i + "+" + j + "=" + num[i][j]);
			}
		}
	}

}
