interface Greet
{
	void greet();
}

interface bye extends Greet
{
	void bye();
}

class Greeting implements Bye
{
	public void greet()
	{
		System.out.println("안녕하세여");
	}
	public void bye();
	{
		System.out.println("안녕히 계세여");
	}
}

public class Meet {

	public static void main(String[] args) {
		Greeting greeting = new Greeting();
		greeting.greet();
		greeting.bye();

	}

}
