package Chapter13_2;

public class Quiz1307 {

	public static void main(String[] args) {
		int[][] arr = new int[2][4];
		
		int num =1;
		
		for(int i = 0; i<2; i++) {
			for(int j=0; j<4; j++) {
				arr[i][j] = num;
				num++;
			}
		}
		
		for(int i = 0; i<4; i++) {
			for(int j=0; j<2; j++) {
				System.out.print(arr[i][j] + "\t");
			}
			System.out.println();
		}
		
	}

}
