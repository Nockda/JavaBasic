//문제1
//길이가 5인 int형 배열을 선언해서 사용자로부터 5개의 정수를 입력받는다.	
//그리고 최대값, 최소값, 모든 수의 합을 구하라. 함수(최대,최소,합)를 정의하여 구현하시오.



import java.util.Scanner;

public class Quiz1301 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int[] cal = new int[5];
		
		cal[0] = sc.nextInt();
		cal[1] = sc.nextInt();
		cal[2] = sc.nextInt();
		cal[3] = sc.nextInt();
		cal[4] = sc.nextInt();
		sc.close();
		
		System.out.println("총 합은 " + (cal[0] + cal[1] + cal[2] + cal[3] + cal[4]));
		
		// 최대값
	        if(cal[0]>=cal[1] && cal[0]>=cal[2] && cal[0]>=cal[3] && cal[0]>=cal[4]) {
		System.out.println("max값은 " + cal[0]);
	}  else if(cal[1]>=cal[2] && cal[1]>=cal[3] && cal[1]>=cal[4]) {
		System.out.println("max값은 " + cal[1]);
	}  else if(cal[2]>=cal[3] && cal[2]>=cal[4]) {
		System.out.println("max값은 " + cal[2]);
	}  else if(cal[3]>=cal[4]) {
		System.out.println("max값은 " + cal[3]);
	}else
	{
		System.out.println("max값은 " + cal[4]);
	}
	
		// 최소값
	      if(cal[0]<=cal[1] && cal[0]<=cal[2] && cal[0]<=cal[3] && cal[0]<=cal[4]) {
		System.out.println("min값은 " + cal[0]);
	}else if(cal[1]<=cal[2] && cal[1]<=cal[3] && cal[1]<=cal[4]) {
		System.out.println("min값은 " + cal[1]);
	}else if(cal[2]<=cal[3] && cal[2]<=cal[4]) {
		System.out.println("min값은 " + cal[2]);
	}else if(cal[3]<=cal[4]) {
		System.out.println("min값은 " + cal[3]);
	}else
	{
		System.out.println("min값은 " + cal[4]);
	}
	
	}

}
